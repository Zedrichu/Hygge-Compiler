fun makeAdder(x: int): () -> (int) -> int = {
    let z = -2;
    fun () -> {
        let z = -1;
        5+5;
        fun (y: int) -> {
           x + 7;
           y + 9;
           z + 11;
           x + y + z // x is captured from the outer surrounding scope, z captured from inner scope
        }
    }
};
