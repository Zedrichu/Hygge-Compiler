// Integer compute-assign
let mutable a = 10;
a += 5;
a -= 3;
a *= 2;
a /= 4;
a %= 3;

// Float compute-assign
let mutable f = 10.5f;
f += 2.5f;
f -= 1.5f;
f *= 2.0f;
f /= 5.0f;

// With expressions
let mutable b = 7;
b += a * 2;
b -= a / 2;
b *= a - 1;

// In control structures
let mutable i = 0;
while (i < 5) do {
    i += 1
};

// Using variables on both sides
let mutable x = 10;
let mutable y = 5;
x += y;

// Nested expressions
let mutable z = 20;
z -= (x + y) * 2